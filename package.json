{
  "name": "cihan-cinar.com",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "classnames": "2.2.6",
    "moment": "2.26.0",
    "sass": "",
    "nouislider": "14.5.0",
    "react": "16.13.1",
    "react-bootstrap-icons": "^1.2.2",
    "react-bootstrap-switch": "15.5.3",
    "react-cookie-consent": "^6.2.3",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-scroll": "^1.8.1",
    "reactstrap": "8.4.1"
